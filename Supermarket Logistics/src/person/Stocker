package person;

import product.Pallet;
import product.Truck;

public class Stocker {

	
	public void removeFromTruck(Truck truck, Pallet pallet) {
		truck.emptyPallet(pallet);
		pallet.gettingTotalAmount();
	}
	
	public void stockShelfFromTruck(Pallet pallet) {
		pallet.stock();
	}
	
	public void stockShelfFromBackstock() {
		
	}
}
